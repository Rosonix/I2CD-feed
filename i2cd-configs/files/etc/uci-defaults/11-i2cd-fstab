cat > /etc/config/fstab <<EOF
config 'global'
        option  anon_swap       '0'
        option  anon_mount      '1'
        option  auto_swap       '1'
        option  auto_mount      '1'
        option  delay_root      '5'
        option  check_fs        '1'

config 'mount'
        option  target  '/mnt/mmcblk0p1'
        option  enabled '1'

config 'mount'
        option  target  '/mnt/mmcblk0p2'
        option  enabled '1'
EOF

exit 0
